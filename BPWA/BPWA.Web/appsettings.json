{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Information",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "AccountLifespan": "30.00:00:00.0000",
    "GuestAccountLifespan": "0.24:00:00.0000"
  },  
  "BackgroundServicesSettings": {
    "AccountDeactivationNotificationsServiceRepeatingPeriod": "0.00:00:05.0000",
    "AccountDeactivationServiceRepeatingPeriod": "0.00:00:05.0000",
    "GuestAccountsDeleterServiceRepeatingPeriod": "0.00:00:05.0000"
  },
  "CacheSettings": {
    "TranslationDuration": "10.00:00:00.0000",
    "CompanyActivityStatusDuration": "10.00:00:00.0000"
  },
  "DatabaseSettings": {
    "AutoMigrate": true,
    "RecreateDatabase": false,
    "Seed": true
  },
  "IdentityOptions": {
    "User": {
      "AllowedUserNameCharacters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-._@+",
      "RequireUniqueEmail": true
    },
    "Password": {
      "RequiredLength": 4,
      "RequiredUniqueChars": 0,
      "RequireNonAlphanumeric": false,
      "RequireLowercase": false,
      "RequireUppercase": false,
      "RequireDigit": false
    },
    "Lockout": {
      "AllowedForNewUsers": false,
      "MaxFailedAccessAttempts": 3,
      "DefaultLockoutTimeSpan": "0.00:05:00.0000"
    },
    "SignIn": {
      "RequireConfirmedEmail": false,
      "RequireConfirmedPhoneNumber": false,
      "RequireConfirmedAccount": false
    },
    "Stores": {
      "MaxLengthForKeys": 1000,
      "ProtectPersonalData": false
    }
  }
}
